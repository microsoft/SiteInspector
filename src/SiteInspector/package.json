{
  "name": "site-inspector",
  "version": "1.0.0",
  "description": "A blade that provides capabilty to add dynamic information about the web page that hosts the blade.",
  "main": "index.jsx",
  "scripts": {
    "build": "set NODE_ENV=production&&webpack -p",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "dev": "webpack -d --watch",
    "lint": "eslint --ext .jsx --ext .js .",
    "lint:fix": "eslint --ext .jsx --ext .js . --fix",
    "prebuild": "npm-run-all clean-dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "start": "babel-node tools/devServer.js",
    "demo": "npm --prefix ../Tabs/CaptureTab install & npm install && npm --prefix ../Tabs/CaptureTab run build && babel-node tools/devServer.js demo",
    "test": "jest",
    "ryan": "npm install; npm run lint",
    "test:watch": "jest --watch"
  },
  "author": "Microsoft",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "es6-promise": "^4.2.4",
    "lodash.assignin": "^4.2.0",
    "lodash.throttle": "^4.1.1",
    "moment": "^2.22.2",
    "office-ui-fabric-react": "^5.54.0",
    "prop-types": "^15.5.10",
    "raf-throttle": "^2.0.3",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^16.4.2",
    "react-modal": "^2.3.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.2.2",
    "recharts": "^1.0.0-beta.10",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "svg-react-loader": "^0.4.6",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "copy-webpack-plugin": "^4.6.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.26.3",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.8.0",
    "jest": "^23.5.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.0.2",
    "react-test-renderer": "^16.4.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^4.1.1",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.13.1",
    "url-loader": "^1.0.1",
    "webpack": "^3.11.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": "7.2.1"
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".*": "./tests/preprocessor.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      ".*.scss"
    ],
    "moduleNameMapper": {
      "^-!babel-loader!svg-react-loader.*$": "<rootDir>/tests/importmock.js",
      "^.*\\.scss$": "<rootDir>/tests/importmock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  }
}
